* {
	margin:0;
	padding:0;
}

html {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  height: 100%;
}

*, *:before, *:after {
  -webkit-box-sizing: inherit;
  -moz-box-sizing: inherit;
  box-sizing: inherit;
  }

*:focus {
    outline: 0 none;
}

a {
	text-decoration: none;
	color: #db3f2a;
	font-weight: 300;
	cursor: pointer;
}

img {
	border: 0;
}

body {
    font: 14px/24px 'Lato', sans-serif;
    font-size: 1em; 
    line-height: 1.2;
    background-color: #f2f2f2;
    height: 100%;
}

svg > g > g:last-child { pointer-events: none } /* flickering de google charts */
div.google-visualization-tooltip { pointer-events: none } /* flickering de google charts */

.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
	clear: both;
	}

.clearfix { *zoom:1;}

.invisible {
	display: none;
}

.alert{
	position: absolute;
	left: 50%;
	transform: translatex(-50%);
	top: 150px;
	background-color: #db3f2a;
	color: #fff;
	padding: 15px 30px;
	display: block;
	z-index: 1002;
	text-transform: uppercase;
	font-weight: 300;
	box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
}

#tooltip
{
    text-align: left;
    color: #fff;
    background: #124c5e;
    position: absolute;
    z-index: 1004;
    padding: 10px;
    font-weight: 300;
    font-size: 0.87em;
    white-space: pre-line;
}
 
    #tooltip:after /* triangle decoration */
    {
        width: 0;
        height: 0;
        border-left: 10px solid transparent;
        border-right: 10px solid transparent;
        border-top: 10px solid #124c5e;
        content: '';
        position: absolute;
        left: 50%;
        bottom: -10px;
        margin-left: -10px;
    }
 
        #tooltip.top:after
        {
            border-top-color: transparent;
            border-bottom: 10px solid #124c5e;
            top: -20px;
            bottom: auto;
        }
 
        #tooltip.left:after
        {
            left: 10px;
            margin: 0;
        }
 
        #tooltip.right:after
        {
            right: 10px;
            left: auto;
            margin: 0;
        }

