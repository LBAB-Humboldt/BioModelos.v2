* {
	margin:0;
	padding:0;
}

html {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  height: 100%;
}

*, *:before, *:after {
  -webkit-box-sizing: inherit;
  -moz-box-sizing: inherit;
  box-sizing: inherit;
  }

*:focus {
    outline: 0 none;
}

a {
	text-decoration: none;
	color: #db3f2a;
	font-weight: 300;
	cursor: pointer;
}

img {
	border: 0;
}

body {
    font: 14px/24px 'Lato', sans-serif;
    font-size: 1em; 
    line-height: 1.2;
    background-color: #f2f2f2;
    height: 100%;
}


.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
	clear: both;
	}

.clearfix { *zoom:1;}

/* header */

header {
	width: 100%;
	background-color: #1b3337;
	padding: 12px 34px;
	display: flex;
    justify-content: space-between;
    position: relative;
    z-index: 12;
}

header h1 {
	font-size: 1.9em;
	color: #fff;
	letter-spacing: 1px;
	font-weight: 400;
}

header nav {
	margin-top: 6px;
}

header nav li {
	list-style: none;
	display: inline-block;
	*display: inline;
	*zoom: 1;;
}

header nav li a {
	text-transform: uppercase;
	padding: 15px;
	color: #fff;
	margin-left: 10px;
	font-size: 0.75em;
	letter-spacing: 1px;
}

header nav a:hover {
	color: #18b4b1;
}

.usuario {
    display: inline-block;
}

.usuario a {
	display: inline-block;
	color:#e45b2d;
	padding: 0 10px;
	text-overflow: ellipsis;
	overflow: hidden;
	max-width: auto;
	vertical-align: text-top;
	cursor: pointer;
	max-width: 220px;
}

.xsalir {
    cursor: pointer;
    width: 16px;
    height: 17px;
    background-image: url(<%= asset_path 'logout.png' %>);
    background-repeat: no-repeat;
    vertical-align: text-bottom;
    margin-left: 10px;
}

.current {
	border: solid 1px #fff;
	color: #18b4b1;
}

.toggle-nav {
	display: none;
}

/* footer */

footer {
	width: 100%;
	background-color: #393836;
	padding: 20px 34px 15px;
	display: flex;
    justify-content: space-between;
    position: static;
    bottom: 0;
    height: 130px;
}

.humboldtft img, 
.humbname {
	display: inline-block;
	*display: inline;
	*zoom: 1;
	margin-right: 10px;
}

.humbname h1 {
	font-size: 12px;
	color: #fff;
	font-weight: 400;
	margin-bottom: 20px;
	font-size: 0.87em;
	letter-spacing: 0.5px;
}

.humbname h2 {
	font-size: 11px;
	color: #fff;
	font-weight: 400;
	font-size: 0.8em;
}

footer nav ul, footer nav {
	display: inline-block;
	*display: inline;
	*zoom: 1;
	vertical-align: top;
}

footer ul:nth-child(2) {
	border-right: solid #75746f 1px;
}
footer ul:nth-child(even) {
	border-left: solid #75746f 1px;
}

footer li {
	list-style: none;
	padding: 0 15px;
	text-transform: uppercase;
	font-size: 0.8em;
	line-height: 1.8;
	width: 105px;
}

footer li a {
	color: #18b4b1;
}

.smallnavli {
	color: #fff;
	text-transform: none;
}

.linehnav {
	line-height: 1.3;
	margin: 3px 0;	
}


/* container */

.innercontainer {
	width: 100%;
	min-height: -moz-calc(100% - 190px);
	min-height: -webkit-calc(100% - 190px);
	min-height: calc(100% - 190px);
	z-index: 10;
	padding-bottom: 30px;
	position: relative;
}

.container {
	margin: 0 auto;
}

.alert{
	position: absolute;
	left: 50%;
	transform: translatex(-50%);
	top: 150px;
	background-color: #db3f2a;
	color: #fff;
	padding: 15px 30px;
	display: block;
	z-index: 11;
	text-transform: uppercase;
	font-weight: 300;
	box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
}

/*home*/

.homeline {
	height: 400px;
	background-image: url(<%= asset_path 'grillo.jpg' %>);
    background-repeat: no-repeat;
  	background-size: cover;
  	background-position: center;
  	opacity: 1;
    -webkit-transition: background 2s linear;
    -moz-transition: background 2s linear;
    -o-transition: background 2s linear;
    transition: background 2s linear;
}

.homec {
	width: 700px;
	position: relative;
	top: 35%;
	transform: translateY(-35%);
}

.homeline form {
	margin: 0 auto;
	width: 100%;
	padding: 5px;
