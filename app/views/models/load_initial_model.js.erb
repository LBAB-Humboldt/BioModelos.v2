<% if @init_model %>
	_BioModelosVisorModule.loadModel('<%= model_layer(@init_model).html_safe %>');
	$(".modelname").show();
	$(".modelname").html("<%= map_status_name(@init_model.modelStatus) %>");
	if(<%= @init_model.modelStatus == 'Developing' %> && !$(".modelname").hasClass("gradient"))
        $(".modelname").addClass("gradient");
<% else %>
	$(".modelname").html("");
	$(".modelname").hide();
<% end %>

<% unless @alert_log.empty? %>
  $("#alertlogbtn").show();
  $('#alertlogbtn').on('click', function() {
    $('#alert_log').html('<%=j render partial: 'layouts/alert_log' %>');
  })
<% end %>
<% unless @alerts_to_show.empty? %>
  $("#alerts").html('<%=j render partial: 'layouts/alert' %>');
<% end %>
